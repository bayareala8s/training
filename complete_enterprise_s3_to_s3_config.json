{
  "transfer_id": "customerA-s3-to-s3-001",
  "source": {
    "type": "s3",
    "bucket": "source-data-bucket",
    "key_prefix": "raw-data/",
    "filter_pattern": "*.csv",
    "region": "us-west-2"
  },
  "destination": {
    "type": "s3",
    "bucket": "target-data-bucket",
    "key_prefix": "processed-data/",
    "region": "us-west-2",
    "add_timestamp_prefix": true,
    "overwrite_existing": false,
    "storage_class": "STANDARD"
  },
  "file_attributes": {
    "min_size_bytes": 1024,
    "max_size_bytes": 524288000,
    "modified_after": "2025-07-01T00:00:00Z",
    "required_extension": ".csv",
    "max_file_age_days": 7,
    "checksum_provided": false,
    "filename_prefix": "data_",
    "filename_suffix": "_ingest",
    "rename_on_upload": {
      "enabled": true,
      "format": "data_{YYYYMMDD_HHmmss}.csv"
    }
  },
  "schedule": {
    "type": "cron",
    "expression": "cron(0 1 * * ? *)",
    "timezone": "UTC"
  },
  "validation": {
    "checksum": "md5",
    "validate_before_upload": true,
    "delete_source_after_success": false
  },
  "notification": {
    "on_success": {
      "email": [
        "ops@customer-a.com",
        "data-eng@company.com"
      ]
    },
    "on_failure": {
      "email": [
        "data-eng@company.com"
      ]
    }
  },
  "logging": {
    "enabled": true,
    "log_group": "/aws/filetransfer/s3-to-s3",
    "log_level": "INFO",
    "track_per_file": true
  },
  "meta": {
    "customer_id": "customer-a",
    "project_code": "data-pipeline",
    "env": "prod",
    "onboarded_by": "team-dataplatform",
    "created_at": "2025-07-16T00:00:00Z"
  },
  "security": {
    "encryption": {
      "s3_encryption": "SSE-KMS",
      "kms_key_arn": "arn:aws:kms:us-west-2:123456789012:key/example-key-id"
    }
  },
  "execution_control": {
    "max_runtime_minutes": 30,
    "backoff_strategy": "exponential",
    "retry_delay_seconds": 60,
    "max_retries": 5,
    "on_timeout_action": "fail"
  },
  "post_processing": {
    "actions": [
      "tag_object",
      "trigger_lambda"
    ],
    "lambda_arn": "arn:aws:lambda:us-west-2:123456789012:function:PostProcessS3ToS3"
  },
  "tags": {
    "department": "data-eng",
    "confidentiality": "medium",
    "owner": "customer-a",
    "transfer_type": "s3-to-s3"
  },
  "monitoring": {
    "metrics_namespace": "FileTransfer",
    "enable_detailed_metrics": true,
    "export_metrics_to_cloudwatch": true
  },
  "workflow": {
    "step_function_arn": "arn:aws:states:us-west-2:123456789012:stateMachine:S3ToS3Flow",
    "priority": "medium",
    "batch_mode": false
  }
}